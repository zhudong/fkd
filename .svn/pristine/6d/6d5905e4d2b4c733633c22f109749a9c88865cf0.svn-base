    <style name="rl_for_login">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="tv_for_login">
        <item name="android:textSize">13sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="DiaLogBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit</item>
    </style>

    <!-- 给Activity用的Dialog类型 -->
    <style name="dialog_down_2_up" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/DiaLogBottom</item>
        <item name="android:windowFrame">@null</item>
        <!--边框-->
        <item name="android:windowIsFloating">true</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <!--无标题-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--背景透明-->
        <item name="android:backgroundDimEnabled">true</item>
        <!--模糊-->
    </style>

    <style name="ed_for_gravity_right_to_normal">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">90dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@null</item>
        <item name="android:numeric">decimal</item>
        <item name="android:shadowColor">@color/white</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">right</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="ed_for_gravity_right_to_pwd_new">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">130dp</item>
        <item name="android:background">@null</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">right</item>
    </style>

    <!-- 这是充值卡订单列表的RidaoButon的style -->
    <style name="gift_card_order_list_rb_style" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/selector_rb_text_for_gift_card_order_list</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@null</item>
    </style>

    <style name="Layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">100px</item>
        <item name="android:background">@color/white</item>
        <item name="android:padding">10dp</item>
    </style>

    <!--仿IOS对话框样式-->
    <style name="mystyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/bg</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="SplashThem" parent="Theme.AppCompat.Light">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="dialog_down_2_up_no_bg" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/DiaLogBottom</item>
        <item name="android:windowFrame">@null</item>
        <!--边框-->
        <item name="android:windowIsFloating">true</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <!--无标题-->
        <item name="android:windowBackground">@color/default_bg</item>
        <!--背景透明-->
        <item name="android:backgroundDimEnabled">false</item>
        <!--模糊-->

    </style>

    <style name="pop_text_style">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="input_box_style">
