<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="133.3dp"
              android:background="@color/white"
              android:minHeight="133.3dp"
              android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_wh_name"
            style="@style/system_Mytextsize_black_13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="18dp"
            android:text="福快递仓库1"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="4dp"
        android:layout_marginTop="9.67dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_address"
            style="@style/system_Mytextsize_black_12_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:lineSpacingExtra="5dp"
            android:text="收货地址:2131232"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            >

            <TextView
                android:id="@+id/tv_receiver"
                style="@style/system_Mytextsize_black_12_gray"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="收货人:2131232"/>

            <TextView
                android:id="@+id/tv_uid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="4076"
                android:textColor="@color/the_red"
                android:textSize="12sp"/>
        </LinearLayout>


        <TextView
            android:id="@+id/tv_phone"
            style="@style/system_Mytextsize_black_12_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:text="电话:2131232"/>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="13.3dp">

        <TextView
            android:id="@+id/tv_post_code"
            style="@style/system_Mytextsize_black_12_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:text="邮编:2131232"/>

        <Button
            android:textAllCaps="false"
            android:id="@+id/trs_copy_button"
            android:layout_width="63.3dp"
            android:layout_height="25.7dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="7.33dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/yellow_bg_cricle"
            android:text="@string/hs_copy_address_btn"
            android:textColor="@color/white"
            android:textSize="12sp"/>
    </RelativeLayout>

</LinearLayout>