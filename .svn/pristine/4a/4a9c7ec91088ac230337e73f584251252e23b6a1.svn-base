<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="wrap_content">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

        <com.fuexpress.kr.ui.view.CustomGridView
                android:id="@+id/gv_help_head"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:horizontalSpacing="2dp"
                android:numColumns="4"
                android:verticalSpacing="8dp"
                />

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@color/white"
                android:orientation="horizontal"
                android:padding="10dp">

            <TextView
                    style="@style/parcel_text_gray_99"
                    android:text="@string/String_parcel_add_photo"/>

            <TextView
                    style="@style/parcel_text_gray_99"
                    android:layout_marginLeft="10dp"
                    android:text="@string/String_parcel_photo_type"/>
        </LinearLayout>

    </LinearLayout>


    <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@mipmap/demand_list_topbg"/>

</FrameLayout>