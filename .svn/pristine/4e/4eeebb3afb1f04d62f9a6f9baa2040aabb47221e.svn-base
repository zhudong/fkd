<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_weight="1"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="3dp"
        android:layout_marginTop="6dp"
        android:background="@color/white"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/iv_in_item_in_choose_self_album"
            android:layout_width="match_parent"
            android:layout_height="110dp"/>

        <TextView
            android:id="@+id/tv_in_item_in_choose_self_album_title"
            style="@style/system_Mytextsize_black_12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:singleLine="true"
            />

        <TextView
            android:id="@+id/tv_in_item_in_choose_self_album_time"
            style="@style/system_Mytextsize_gray_11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            android:singleLine="true"
            />
    </LinearLayout>


    <ImageView
        android:id="@+id/iv_in_item_in_choose_self_album_is_selected"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:src="@mipmap/unselect"/>


</FrameLayout>