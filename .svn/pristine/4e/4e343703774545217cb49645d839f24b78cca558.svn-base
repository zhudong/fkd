<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:longer="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_bg"
    android:orientation="vertical"
    >

    <com.fuexpress.kr.ui.view.TitleBarView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/title_in_my_balance_account"
        longer:text="@string/string_for_my_balance_rl_01"/>

    <com.fuexpress.kr.ui.view.RefreshListView
        android:id="@+id/rfl_in_gift_card_account"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:divider="@null">

    </com.fuexpress.kr.ui.view.RefreshListView>

</LinearLayout>
