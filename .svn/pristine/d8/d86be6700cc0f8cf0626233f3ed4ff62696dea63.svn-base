<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/default_bg"
    xmlns:zhudong="http://schemas.android.com/apk/res-auto">

    <com.fuexpress.kr.ui.view.TitleBarView
        android:id="@+id/integral_management_titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        zhudong:text="@string/integral_management_msg"/>

    <ListView
        android:layout_marginTop="5dp"
        android:id="@+id/integral_management_list_view"
        android:layout_width="match_parent"
        android:divider="@color/gary_e8e8"
        android:dividerHeight="1dp"
        android:layout_height="wrap_content" />
</LinearLayout>
