public class IDinfoBean {
    boolean mNeedId;
    String idNumber;
    String urlFront;
    String urlBack;


    String serverIDNumber;
    String serverUrlFront;
    String serverUrlBack;

    public IDinfoBean(boolean needId, String idNumber, String urlFront, String urlBack, String serverIDNumber, String serverUrlFront, String serverUrlBack) {
        mNeedId = needId;
        this.idNumber = idNumber;
        this.urlFront = urlFront;
        this.urlBack = urlBack;
        this.serverIDNumber = serverIDNumber;
        this.serverUrlFront = serverUrlFront;
        this.serverUrlBack = serverUrlBack;
    }

    public String getServerIDNumber() {
        return serverIDNumber;
    }

    public void setServerIDNumber(String serverIDNumber) {
        this.serverIDNumber = serverIDNumber;
    }

    public String getServerUrlFront() {
        return serverUrlFront;
    }

    public void setServerUrlFront(String serverUrlFront) {
        this.serverUrlFront = serverUrlFront;
    }

    public String getServerUrlBack() {
        return serverUrlBack;
    }

    public void setServerUrlBack(String serverUrlBack) {
        this.serverUrlBack = serverUrlBack;
    }

    public IDinfoBean() {
    }

    public IDinfoBean(boolean needId, String idNumber, String urlFront, String urlBack) {
        mNeedId = needId;
        this.idNumber = idNumber;
        this.urlFront = urlFront;
        this.urlBack = urlBack;
    }

    public boolean isNeedId() {
        return mNeedId;
    }

    public void setNeedId(boolean needId) {
        mNeedId = needId;
    }

    public String getIdNumber() {
        return idNumber;
    }

    public void setIdNumber(String idNumber) {
        this.idNumber = idNumber;
    }

    public String getUrlFront() {
        return urlFront;
    }

    public void setUrlFront(String urlFront) {
        this.urlFront = urlFront;
    }

    public String getUrlBack() {
        return urlBack;
    }

    public void setUrlBack(String urlBack) {
        this.urlBack = urlBack;
    }
}
