<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:longer="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent">
        <com.fuexpress.kr.ui.view.TitleBarView
            android:id="@+id/tl_in_help_get_package"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            longer:text="@string/home_fg_help_02"/>

        <include layout="@layout/demand_head_layout"/>

        <ListView
            android:id="@+id/lv_hs_preview"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="1dp"
            android:layout_weight="1"
            android:divider="@color/default_bg"
            android:dividerHeight="1dp"/>

        <include layout="@layout/footer_sub_demand"/>
    </LinearLayout>

</FrameLayout>