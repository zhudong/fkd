<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:longer="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_bg"
    android:orientation="vertical">

    <com.fuexpress.kr.ui.view.TitleBarView
        android:id="@+id/title_in_search_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        longer:text="@string/string_search_result_title"/>

    <com.fuexpress.kr.ui.view.RefreshListView
        android:id="@+id/rfl_in_search_result"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/transparent"
        android:dividerHeight="10dp"/>


</LinearLayout>
