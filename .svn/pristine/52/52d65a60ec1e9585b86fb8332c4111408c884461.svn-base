<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:longer="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_bg"
    android:orientation="vertical"
    >

    <com.fuexpress.kr.ui.view.TitleBarView
        android:id="@+id/title_in_my_balance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        longer:text="@string/string_for_my_balance_title"/>

    <RelativeLayout
        android:id="@+id/rl_in_gift_card_Account"
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:layout_marginTop="13dp"
        android:background="@color/white"
        >

        <TextView

            style="@style/system_Mytextsize_black_13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:text="@string/string_for_my_balance_rl_01"/>


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="7dp"
            android:src="@mipmap/arrow_right_for_uid"/>
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/default_bg"/>

    <RelativeLayout
        android:id="@+id/rl_in_gift_card_oder_check"
        android:layout_width="match_parent"
        android:layout_height="49dp"

        android:background="@color/white"
        >

        <TextView
            style="@style/system_Mytextsize_black_13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:text="@string/string_for_my_balance_rl_02"/>


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="7dp"
            android:src="@mipmap/arrow_right_for_uid"/>
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/default_bg"/>

    <RelativeLayout
        android:id="@+id/rl_in_gift_card_bander"
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:background="@color/white"
        >

        <TextView
            style="@style/system_Mytextsize_black_13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:text="@string/my_gift_card_tab_03"/>


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="7dp"
            android:src="@mipmap/arrow_right_for_uid"/>
    </RelativeLayout>

</LinearLayout>
