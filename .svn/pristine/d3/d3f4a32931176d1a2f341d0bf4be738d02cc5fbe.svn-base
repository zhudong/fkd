<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/default_bg"
    >

    <ImageView
        android:id="@+id/iv_empty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="130dp"
        android:src="@mipmap/gift_card_order_list_empty"/>

    <TextView
        style="@style/system_Mytextsize_GRAY_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_empty"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:text="@string/my_gift_card_order_no_order_text"/>

    <TextView
        android:id="@+id/tv_in_gift_card_order_list_empty"
        style="@style/system_Mytextsize_black_13"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_below="@+id/iv_empty"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:background="@drawable/shape_bg_for_home_head_5px"
        android:gravity="center"
        android:text="@string/my_gift_card_order_no_order_text_02"/>

</RelativeLayout>