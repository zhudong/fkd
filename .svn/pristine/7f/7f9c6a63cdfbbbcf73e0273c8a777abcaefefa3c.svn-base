<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="69dp"
              android:background="@color/white"
              android:orientation="horizontal">

    <ImageView
        android:id="@+id/iv_in_item_hs"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="10dp"/>

    <RelativeLayout
        android:layout_marginLeft="9.3dp"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/rl_title_contair"
            android:layout_width="232.6dp"
            android:layout_height="match_parent"
            >

            <TextView
                android:id="@+id/tv_hs_item_title"
                style="@style/system_Mytextsize_black_13"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="10dp"
                android:text="12345667"/>

            <TextView
                android:id="@+id/tv_hs_item_price"
                style="@style/system_Mytextsize_black_13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_hs_item_title"
                android:layout_marginLeft="7dp"
                android:text="W123456"/>


        </RelativeLayout>


        <TextView
            android:id="@+id/tv_hs_item_qty"
            style="@style/system_Mytextsize_black_13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="17.3dp"
            android:layout_marginTop="34dp"
            android:layout_toRightOf="@+id/rl_title_contair"
            android:text="X1"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="11.7dp"
            android:layout_marginTop="34dp"
            android:src="@mipmap/arrow_right"/>
    </RelativeLayout>
</LinearLayout>